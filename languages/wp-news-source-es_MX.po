msgid "Total sources: %d"
msgstr "Total de fuentes: %d"

msgid "API Endpoints for n8n"
msgstr "Endpoints de API para n8n"

msgid "Use these endpoints in your n8n workflow:"
msgstr "Usa estos endpoints en tu flujo de trabajo de n8n:"

msgid "Get complete mapping"
msgstr "Obtener mapeo completo"

msgid "Detect source (POST)"
msgstr "Detectar fuente (POST)"

msgid "List all sources"
msgstr "Listar todas las fuentes"

# Messages
msgid "Source saved successfully"
msgstr "Fuente guardada correctamente"

msgid "Action completed successfully."
msgstr "Acción completada correctamente."

msgid "You do not have permission to access this page."
msgstr "No tienes permisos para acceder a esta página."

msgid "You do not have permission to perform this action"
msgstr "No tienes permisos para realizar esta acción"

msgid "Source not found."
msgstr "Fuente no encontrada."

